{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "ignorePatterns": ["dist", "node_modules", "webpack*.js"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "camelcase": "off"
    }
}
